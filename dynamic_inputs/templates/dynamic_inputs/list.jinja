{% extends "dynamic_inputs/base.jinja" %}
{% block content %}
    <div class="list-group">
        {% for entry in entrys %}
            <a href="{{ entry.get_absolute_url() }}"
               class="list-group-item list-group-item-action">{{ entry.id }}</a>
        {% endfor %}
    </div>
    <hr class="my-4">
    <a href="{{ url_for('dynamic_inputs.add') }}" class="btn btn-primary">Add new</a>
{% endblock content %}
